<h2>List of Users</h2>

<table class='table'>
    <tr>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined</th>
    </tr>

     
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.username, user_path(user) %></td>
        <td> <%= user.votes.count %></td>
        <td><%= user.created_at.strftime("%m/%d/%Y") %> </td>
      </tr>
    <% end %>
  </table> 

<%= link_to 'Back to Media List', works_path, class: "btn btn-secondary" %>

